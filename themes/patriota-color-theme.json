{
	"name": "patriota",
	"colors": {
		"activityBar.background": "#009c3b",
		"activityBar.activeBackground": "#002776",
		"activityBar.activeBorder": "#ffdf00",
		"activityBar.activeFocusBorder": "#ffdf00",
		"activityBar.foreground": "#ffffff",
		"activityBar.inactiveForeground": "#ffffffaa",
		"activityBarBadge.background": "#ffdf00",
		"activityBarBadge.foreground": "#002776",

		"sideBar.background": "#002776",
		"sideBar.foreground": "#ffffff",
		"sideBarTitle.foreground": "#ffdf00",
		"sideBarSectionHeader.background": "#002776",
		"sideBarSectionHeader.foreground": "#ffdf00",

		"editor.background": "#002776",
		"editor.foreground": "#ffffff",
		"editorLineNumber.foreground": "#ffffff50",
		"editorLineNumber.activeForeground": "#009c3b",
		"editorCursor.foreground": "#ffdf00",
		"editor.selectionBackground": "#009c3b55",
		"editor.lineHighlightBackground": "#009c3b22",

		"statusBar.background": "#009c3b",
		"statusBar.foreground": "#ffffff",
		"statusBar.debuggingBackground": "#ffdf00",
		"statusBar.debuggingForeground": "#002776",
		"statusBar.noFolderBackground": "#002776",

		"titleBar.activeBackground": "#002776",
		"titleBar.activeForeground": "#ffdf00",
		"titleBar.inactiveBackground": "#002776",
		"titleBar.inactiveForeground": "#ffffff88",

		"list.activeSelectionBackground": "#009c3b",
		"list.activeSelectionForeground": "#ffffff",
		"list.focusBackground": "#009c3b",
		"list.hoverBackground": "#009c3b44",
		"list.inactiveSelectionBackground": "#009c3b88",

		"tab.activeBackground": "#002776",
		"tab.activeForeground": "#ffdf00",
		"tab.inactiveBackground": "#002776",
		"tab.inactiveForeground": "#ffffffaa",
		"tab.activeBorderTop": "#ffdf00",

		"input.background": "#002776",
		"input.foreground": "#ffffff",
		"input.border": "#009c3b",
		"input.placeholderForeground": "#ffffff88",

		"terminal.foreground": "#ffffff",
		"terminal.background": "#002776",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiRed": "#ff5555",
		"terminal.ansiGreen": "#009c3b",
		"terminal.ansiYellow": "#ffdf00",
		"terminal.ansiBlue": "#4da6ff",
		"terminal.ansiMagenta": "#ffffff",
		"terminal.ansiCyan": "#ffffff",
		"terminal.ansiWhite": "#ffffff",
		"terminal.ansiBrightBlack": "#ffffff55",
		"terminal.ansiBrightRed": "#ff5555",
		"terminal.ansiBrightGreen": "#009c3b",
		"terminal.ansiBrightYellow": "#ffdf00",
		"terminal.ansiBrightBlue": "#4da6ff",
		"terminal.ansiBrightMagenta": "#ffffff",
		"terminal.ansiBrightCyan": "#ffffff",
		"terminal.ansiBrightWhite": "#ffffff",

		"progressBar.background": "#ffdf00",
		"focusBorder": "#009c3b",
		"pickerGroup.border": "#009c3b",
		"pickerGroup.foreground": "#ffdf00",

		"debugToolBar.background": "#002776",
		"button.background": "#009c3b",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#002776",

		"dropdown.background": "#002776",
		"dropdown.border": "#009c3b",
		"dropdown.foreground": "#ffffff"
	},
	"semanticHighlighting": true,
	"tokenColors": [
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#ffdf00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Control Keywords",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#ffdf00",
				"fontStyle": "bold italic"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#009c3b"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#009c3b",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"support.constant"
			],
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Numbers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Booleans",
			"scope": "constant.language.boolean",
			"settings": {
				"foreground": "#ffdf00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Classes/Types",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Tags",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": "source.json meta.structure.dictionary.json support.type.property-name.json",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Markdown - Headings",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#ffdf00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown - Links",
			"scope": [
				"markup.underline.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		}
	]
}
