{
	"name": "patriota",
	"colors": {
		"activityBar.background": "#009c3b",
		"activityBar.activeBackground": "#007a2d",
		"activityBar.activeBorder": "#ffdf00",
		"activityBar.activeFocusBorder": "#ffdf00",
		"activityBar.foreground": "#ffffff",
		"activityBar.inactiveForeground": "#e1e4e8aa",
		"activityBarBadge.background": "#ffdf00",
		"activityBarBadge.foreground": "#002776",

		"sideBar.background": "#002776",
		"sideBar.foreground": "#ffffff",
		"sideBarTitle.foreground": "#ffdf00",
		"sideBarSectionHeader.background": "#001b52",
		"sideBarSectionHeader.foreground": "#ffdf00",

		"editor.background": "#00152e",
		"editor.foreground": "#e1e4e8",
		"editorLineNumber.foreground": "#ffffff50",
		"editorLineNumber.activeForeground": "#ffdf00",
		"editorCursor.foreground": "#ffdf00",
		"editor.selectionBackground": "#009c3b55",
		"editor.lineHighlightBackground": "#009c3b22",

		"statusBar.background": "#009c3b",
		"statusBar.foreground": "#ffffff",
		"statusBar.debuggingBackground": "#ffdf00",
		"statusBar.debuggingForeground": "#002776",
		"statusBar.noFolderBackground": "#002776",

		"titleBar.activeBackground": "#002776",
		"titleBar.activeForeground": "#ffdf00",
		"titleBar.inactiveBackground": "#001b52",
		"titleBar.inactiveForeground": "#ffffff88",

		"list.activeSelectionBackground": "#009c3b",
		"list.activeSelectionForeground": "#ffffff",
		"list.focusBackground": "#009c3b",
		"list.hoverBackground": "#ffffff22",
		"list.inactiveSelectionBackground": "#009c3b88",

		"tab.activeBackground": "#00152e",
		"tab.activeForeground": "#ffdf00",
		"tab.inactiveBackground": "#002776",
		"tab.inactiveForeground": "#ffffffaa",
		"tab.activeBorderTop": "#ffdf00",

		"input.background": "#00152e",
		"input.foreground": "#e1e4e8",
		"input.border": "#009c3b",
		"input.placeholderForeground": "#ffffff88",

		"terminal.foreground": "#e1e4e8",
		"terminal.background": "#00152e",

		"progressBar.background": "#ffdf00",
		"focusBorder": "#009c3b",
		"pickerGroup.border": "#009c3b",
		"pickerGroup.foreground": "#ffdf00",

		"debugToolBar.background": "#002776",
		"button.background": "#009c3b",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#007a2d",

		"dropdown.background": "#00152e",
		"dropdown.border": "#009c3b",
		"dropdown.foreground": "#e1e4e8"
	},
	"semanticHighlighting": true,
	"tokenColors": [
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#ffdf00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#00ffa2"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#009c3b"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#6a737d",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"support.constant"
			],
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Classes/Types",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#4da6ff"
			}
		},
		{
			"name": "Tags",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#00ffa2"
			}
		}
	]
}
