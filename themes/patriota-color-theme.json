	"semanticHighlighting": true,
	"tokenColors": [
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"variable.language"
			],
			"settings": {
				"foreground": "#ffdf00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Control Keywords",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#ffdf00",
				"fontStyle": "bold italic"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#009c3b"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#009c3b",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"support.constant"
			],
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Numbers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Booleans",
			"scope": "constant.language.boolean",
			"settings": {
				"foreground": "#ffdf00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Classes/Types",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Tags",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": "source.json meta.structure.dictionary.json support.type.property-name.json",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Markdown - Headings",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"foreground": "#ffdf00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown - Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Markdown - Links",
			"scope": [
				"markup.underline.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Markdown - Blockquote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#009c3b",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff5555"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": [
				"markup.italic",
				"markup.italic string"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Markup - Inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#009c3b"
			}
		},
		{
			"name": "Markup - Deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#ff5555"
			}
		},
		{
			"name": "Markup - Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#ffdf00"
			}
		}
	]
}
